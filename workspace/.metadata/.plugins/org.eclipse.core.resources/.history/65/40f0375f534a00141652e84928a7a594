#include <iostream>
#include "DLList.h"
using namespace std;
using namespace ods;

int main() {

	DLList<string> dllist;
	dllist.add("atoyotaisatoyota");

	cout << "is this a palindrome? " << dllist.IsPalindrome() << endl;
/*
	dllist.clear();
	dllist.add("anna");
	cout << "is this a palindrome? " << dllist.IsPalindrome() << endl;
*/
	dllist.clear();
	dllist.add("irina");
	cout << "is this a palindrome? " << dllist.IsPalindrome() << endl;

	return 0;
}
